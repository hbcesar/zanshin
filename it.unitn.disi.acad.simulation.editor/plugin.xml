<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="it.unitn.disi.acad.model.acad.presentation.AcadModelWizardID"
            name="%_UI_AcadModelWizard_label"
            class="it.unitn.disi.acad.model.acad.presentation.AcadModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/AcadModelFile.gif">
         <description>%_UI_AcadModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="it.unitn.disi.acad.model.acad.presentation.AcadEditorID"
            name="%_UI_AcadEditor_label"
            icon="icons/full/obj16/AcadModelFile.gif"
            extensions="acad"
            class="it.unitn.disi.acad.model.acad.presentation.AcadEditor"
            contributorClass="it.unitn.disi.acad.model.acad.presentation.AcadActionBarContributor">
      </editor>
   </extension>

</plugin>
